{
  "expo": {
    "name": "SIGNify",
    "slug": "SIGNify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/signify.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "sdkVersion" : "39.0.0",
