    "android": {
      "package": "com.mdsiddiq.SIGNify",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/signify.png",
        "backgroundColor": "#FFFFFF"
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
